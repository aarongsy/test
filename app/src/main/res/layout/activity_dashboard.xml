<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.diana.test3.Me"
    android:background="@drawable/background2">



    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BACK"
        android:id="@+id/button9"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HOME"
        android:id="@+id/button10"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="   "
        android:id="@+id/textView7"
        android:textColor="#ff020202"
        android:textSize="@dimen/new3"
        android:layout_marginTop="8dp"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:text="Me"
        android:id="@+id/button11"
        android:background="@drawable/roundbutton"
        android:layout_marginLeft="35dp"
        android:textSize="@dimen/new3"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"/>

    <Button
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:text="Request"
        android:id="@+id/button12"
        android:background="@drawable/roundbutton"
        android:textSize="@dimen/new4"
        android:layout_marginLeft="1dp"
        android:layout_above="@+id/button11"
        android:layout_toStartOf="@+id/textView7" />

    <Button
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:text="Status"
        android:id="@+id/button13"
        android:background="@drawable/roundbutton"
        android:textSize="@dimen/new4"
        android:layout_above="@+id/button11"
        android:layout_toEndOf="@+id/textView7" />

    <Button
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:text="Report"
        android:id="@+id/button14"
        android:background="@drawable/roundbutton"
        android:layout_marginLeft="1dp"
        android:textSize="@dimen/new4"
        android:layout_below="@+id/button11"
        android:layout_alignStart="@+id/button12" />

    <Button
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:text="About us"
        android:id="@+id/button15"
        android:background="@drawable/roundbutton"
        android:textSize="@dimen/new4"
        android:layout_below="@+id/button11"
        android:layout_alignStart="@+id/button13" />

</RelativeLayout>
